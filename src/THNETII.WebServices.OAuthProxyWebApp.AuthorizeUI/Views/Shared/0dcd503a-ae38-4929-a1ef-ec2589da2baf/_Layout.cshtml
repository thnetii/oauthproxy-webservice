<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - @ViewData["AppName"]</title>
    <environment include="Development">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/css/bootstrap-grid.css"
              integrity="sha256-JtktgiuQAd+AXerCnPMrHCDz1h5AtkH5tobvpuG7xZ4="
              asp-fallback-href="~/@ViewData["StaticWebAssetBasePath"]/lib/bootstrap/dist/css/bootstrap-grid.css"
              asp-fallback-test-class="d-block" asp-fallback-test-property="display" asp-fallback-test-value="block"
              crossorigin="anonymous" referrerpolicy="no-referrer" />
        <link rel="stylesheet" href="~/@ViewData["StaticWebAssetBasePath"]/css/fast-design-system.css" />
        <link rel="stylesheet" href="https://static2.sharepointonline.com/files/fabric/office-ui-fabric-core/11.0.0/css/fabric.css"
              asp-fallback-href="~/@ViewData["StaticWebAssetBasePath"]/lib/office-ui-fabric-core/css/fabric.css"
              asp-fallback-test-class="ms-textTruncate" asp-fallback-test-property="text-overflow" asp-fallback-test-value="ellipsis"
              crossorigin="anonymous" referrerpolicy="no-referrer" />
        <link rel="stylesheet" href="~/@ViewData["StaticWebAssetBasePath"]/lib/microsoft/fluentui-system-icons/fonts/FluentSystemIcons-Regular.css"
              crossorigin="anonymous" referrerpolicy="no-referrer" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/css/bootstrap-grid.min.css"
              integrity="sha256-ysBT/JYxH9gcMnwxT4+MB4sPxOx/JMg9wi77FA13T9A="
              asp-fallback-href="~/@ViewData["StaticWebAssetBasePath"]/lib/bootstrap/dist/css/bootstrap-grid.min.css"
              asp-fallback-test-class="d-block" asp-fallback-test-property="display" asp-fallback-test-value="block"
              crossorigin="anonymous" referrerpolicy="no-referrer" />
        <link rel="stylesheet" href="~/@ViewData["StaticWebAssetBasePath"]/css/fast-design-system.min.css" />
        <link rel="stylesheet" href="https://static2.sharepointonline.com/files/fabric/office-ui-fabric-core/11.0.0/css/fabric.min.css"
              asp-fallback-href="~/@ViewData["StaticWebAssetBasePath"]/lib/office-ui-fabric-core/css/fabric.min.css"
              asp-fallback-test-class="ms-textTruncate" asp-fallback-test-property="text-overflow" asp-fallback-test-value="ellipsis"
              crossorigin="anonymous" referrerpolicy="no-referrer" />
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/microsoft/fluentui-system-icons@1.1.139/fonts/FluentSystemIcons-Regular.min.css"
              crossorigin="anonymous" referrerpolicy="no-referrer" />
    </environment>
</head>
<body>
    <main role="main" class="pb-3">
        @RenderBody()
    </main>

    <environment include="Development">
        <script type="module" src="~/@ViewData["StaticWebAssetBasePath"]/lib/@('@')fluentui/web-components/dist/web-components.js"></script>
    </environment>
    <environment exclude="Development">
        <script type="module" src="https://cdn.jsdelivr.net/npm/@@fluentui/web-components@1.3.5/dist/web-components.min.js"
                integrity="sha256-iqxWwQFDQKz6csx34jRlKd/7Dq+hrZ8LFMU16NV0M24="
                crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    </environment>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
