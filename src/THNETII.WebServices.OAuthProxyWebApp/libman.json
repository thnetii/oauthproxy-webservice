{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "provider": "jsdelivr",
      "library": "bootstrap@5.1.0",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "jsdelivr",
      "library": "@fluentui/web-components@1.3.5",
      "destination": "wwwroot/lib/@fluentui/web-components/"
    },
    {
      "provider": "filesystem",
      "library": "https://cdn.jsdelivr.net/gh/microsoft/fluentui-system-icons@1.1.137/fonts/FluentSystemIcons-Regular.css",
      "destination": "wwwroot/lib/microsoft/fluentui-system-icons/fonts"
    },
    {
      "provider": "filesystem",
      "library": "https://cdn.jsdelivr.net/gh/microsoft/fluentui-system-icons@1.1.137/fonts/FluentSystemIcons-Regular.html",
      "destination": "wwwroot/lib/microsoft/fluentui-system-icons/fonts"
    },
    {
      "provider": "filesystem",
      "library": "https://cdn.jsdelivr.net/gh/microsoft/fluentui-system-icons@1.1.137/fonts/FluentSystemIcons-Regular.json",
      "destination": "wwwroot/lib/microsoft/fluentui-system-icons/fonts"
    },
    {
      "provider": "filesystem",
      "library": "https://cdn.jsdelivr.net/gh/microsoft/fluentui-system-icons@1.1.137/fonts/FluentSystemIcons-Regular.ttf",
      "destination": "wwwroot/lib/microsoft/fluentui-system-icons/fonts"
    },
    {
      "provider": "filesystem",
      "library": "https://cdn.jsdelivr.net/gh/microsoft/fluentui-system-icons@1.1.137/fonts/FluentSystemIcons-Regular.min.css",
      "destination": "wwwroot/lib/microsoft/fluentui-system-icons/fonts"
    }
  ]
}
